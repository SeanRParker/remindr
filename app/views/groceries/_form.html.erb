<%= form_for @grocery do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name, autofocus: true %>

    <%= f.label :completed %>
		<%= f.check_box :completed %>

    <%= f.submit class: "green darken-4 white-text waves-effect waves-light btn"%>

    <% f.hidden_field :user_id, :value => current_user.id %>

<% end %>
